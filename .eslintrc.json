{
  "extends": [
    "next",
    "next/core-web-vitals",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "tabWidth": 2,
        "printWidth": 158,
        "singleQuote": true
      }
    ],
    "react/jsx-curly-brace-presence": [
      "error",
      {
        "props": "always"
      }
    ]
  }
}